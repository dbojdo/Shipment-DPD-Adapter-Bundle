{
    "name": "webit/shipment-dpd-adapter-bundle",
    "type": "symfony-bundle",
    "description": "Web-IT Shipment DPD Adapter Symfony integration",
    "keywords": [
        "web-it",
        "shipment",
        "bundle",
        "delivery",
        "parcel",
        "dispatch",
        "dpd"
    ],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "webit/shipment-dpd-adapter": "^1.0.0",
        "webit/shipment-bundle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\ShipmentDpdAdapter\\": ""
        }
